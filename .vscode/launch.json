  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Active Target",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${command:cmake.launchTargetDirectory}",
      "environment": [
        {
          "name": "PATH",
          "value": "$PATH:${command:cmake.launchTargetDirectory}"
        },
      ],
      "console": "internalConsole"
    }
  ]
}